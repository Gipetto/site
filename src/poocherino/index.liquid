<body id="poocherino" class="poocherino">
    <header>
        <h1>Poocherino</h1>
    </header>
    <main>
        <div class="paw">
            {% render "./assets/paw.svg" %}
        </div>
        {% render "flickr-album" with flickr.albums["72157603367803058"] as album %}
    </main>
    <footer>
        <p>&copy; <a href="https://top-frog.com">Shawn Parker &mdash; top-frog.com</a></p>
    </footer>
    <dialog class="lightbox">
        <form method="dialog">
          <button class="sr-only" autofocus formmethod="dialog">
            <span>Close Dialog</span>
          </button>
          <img eleventy:ignore class="" src="">
        </form>
      </dialog>
    <script type="text/javascript" src="./js/main.js?c={{ site.buildTime }}"></script>
